{
  "version": "1.0.2",
  "description": "Contains registration and configuration APIs to add the core framework encoders to a dependency injection container.",
  "packOptions": {
    "repository": {
      "type": "git",
      "url": "git://github.com/aspnet/httpabstractions"
    },
    "tags": [
      "aspnetcore"
    ]
  },
  "buildOptions": {
    "warningsAsErrors": true,
    "allowUnsafe": true,
    "keyFile": "../../tools/Key.snk",
    "nowarn": [
      "CS1591"
    ],
    "xmlDoc": true
  },
  "dependencies": {
    "Microsoft.Extensions.DependencyInjection.Abstractions": "1.0.1",
    "Microsoft.Extensions.Options": "1.0.2",
    "System.Text.Encodings.Web": "4.0.0"
  },
  "frameworks": {
    "netstandard1.0": {}
  }
}